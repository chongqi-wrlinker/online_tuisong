.xuanfu{
    height: 1rpx;
    width:100%;
    position: fixed;
    top:40%;
    left: 0px;
    z-index: 10004;
}

.xuanfu image{
    height: 100%;
    width: 100%;
}
.xuanfu .leftArrow{
    height: 100rpx;
    width: 100rpx;
    float: left;
    background-color: #e9dfc7
}
.xuanfu .rightArrow{
    height: 100rpx;
    width: 100rpx;
    float: right;
    background-color: #e9dfc7
}
/*悬浮分享样式**/
.share{
    position:fixed;
    top:26rpx;
    right:5px;
    width:70rpx;
    height:70rpx;
    z-index: 10004
}
.shareBtn{
    background-image: url(data:image/png;base64,);
    width:100%;
    height:100%;
    background-size:100% 100%;
    background-color:#e9dfc7;
    border:0px;
    outline:none;

}
.share image{
    width: 100%;
    height: 100%;
}